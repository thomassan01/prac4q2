    	return numberOfQuiz;
    }
    
    pulbic int getScore(){
    	return score;
    }
    
    public void setStudID(String studentID){
    	studID = studentID;
    }
    
    public void setStudName(String studentName){
    	name = studentName;
    }
    
    public void addQuiz(int total){
    	number = 0;
    	for (int i = 0 ; i <= total.length ; i++){
    		
    		if ( score == 1 ){
    			number++;
    		}
    	}
    }
    
	public double getAverageScore(){
        return totalScore/(double)quizCount;
    }
    
}
